            "sin(pi/8) con polinomio interpolador: 0.41821\n",
            "Original sin(pi/8): 0.38268\n",
            "Error en la estimación: 0.03553\n"
          ]
        }
      ],
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "eje_x = np.array([0, np.pi/3, np.pi/2])\n",
        "eje_y = np.sin(eje_x)\n",
        "\n",
        "\n",
        "def polinomio_interpolador(x, xdat, ydat):\n",
        "    recorrido = len(xdat)\n",
        "    p = np.zeros_like(x)\n",
        "    for i in range(recorrido):\n",
        "        novo = np.ones_like(x)\n",
        "        for j in range(recorrido):\n",
        "            if i != j:\n",
        "                novo *= (x - xdat[j]) / (xdat[i] - xdat[j])\n",
        "        p += ydat[i] * recorrido\n",
        "    return p\n",
        "\n",
        "\n",
        "valoresx = np.linspace(0, np.pi/2, 100)\n",
        "valoresy= polinomio_interpolador(valoresx,eje_x , eje_y)\n",
        "\n",
        "estimacion_x = np.pi / 8\n",
        "estimacion_y = polinomio_interpolador(np.array([estimacion_x]), eje_x, eje_y)[0]\n",
        "\n",
        "\n",
        "y_original = np.sin(estimacion_x)\n",
        "\n",
        "error = np.abs(y_original - estimacion_y)\n",
        "\n",
        "\n",
        "plt.plot(valoresx, valoresy)\n",
        "plt.scatter(eje_x, eje_y)\n",
        "plt.scatter(estimacion_x, estimacion_y, label=f' sin(pi/8) ≈ {estimacion_y:.5f}')\n",
        "plt.axhline(y_original,  label=f'verdadero sin(pi/8) ≈ {y_original:.5f}')\n",
        "plt.grid(True)\n",
        "plt.show()\n",
        "\n",
        "print(f\" sin(pi/8) con polinomio interpolador: {estimacion_y:.5f}\")\n",
        "print(f\"original sin(pi/8): {y_original:.5f}\")\n",
        "print(f\"Error en la estimación: {error:.5f}\")\n"
      ]
    }
  ]
